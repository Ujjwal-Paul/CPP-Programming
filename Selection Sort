int Find_Min(vector<int>& Num,int index){
    int minValue = Num[index];
    for(int i=index+1;i<Num.size();i++){
        if(Num[i] < minValue){
            minValue = Num[i];
            index = i;
        }
    }
    return index;
}

void Selection_Sort(vector<int>& Num){
    for(int i=0;i<Num.size();i++){
        int minIndex = Find_Min(Num,i);
        int Swap = Num[i];
        Num[i] = Num[minIndex];
        Num[minIndex] = Swap;
    }
}
